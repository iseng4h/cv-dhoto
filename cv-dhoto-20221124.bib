   keywords = {Air quality,Cloud computing,Data abstraction,Internet of things,Kafka},
   pages = {1520-1527},
   publisher = {Insight Society},
   title = {Implementation of data abstraction layer using kafka on SEMAR platform for air quality monitoring},
   volume = {9},
   year = {2019},
}
